{
  "name": "beautyincode.se",
  "version": "0.2.0",
  "description": "Web site for Beauty In Code",
  "main": "index.html",
  "scripts": {
    "prestart": "yarn",
    "start": "concurrently 'yarn run watch' 'yarn run jekyll:serve'",
    "prebuild": "yarn run dist:clean",
    "watch": "onchange ./src/svg/* -i -- yarn run svg",
    "svg": "svg-sprite -C svg-sprite.config.json --dest ./_includes svg/*.svg",
    "jekyll:serve": "sleep 1; jekyll serve --incremental",
    "jekyll:build": "jekyll build --incremental"
  },
  "author": "Fredrik Frodlund",
  "license": "UNLICENSED",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "onchange": "^5.2.0",
    "svg-sprite": "^1.3.7",
    "yarn": "^1.17.3"
  }
}
